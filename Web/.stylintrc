{
  "borderNone": true,
  "brackets": true,
  "colons": true,
  "colors": true,
  "commaSpace": true,
  "commentSpace": true,
  "cssLiteral": true,
  "depthLimit": 3,
  "duplicates": true,
  "efficient": true,
  "emoji": true,
  "enforceBlockStyle": true,
  "enforceVarStyle": true,
  "extendPref": true,
  "globalDupe": true,
  "indentSpaces": 2,
  "leadingZero": true,
  "maxWarnings": 10,
  "maxWarningsKill": true,
  "mixed": true,
  "namingConvention": "lowercase-dash",
  "namingConventionStrict": true,
  "parenSpace": true,
  "placeholders": true,
  "quotePref": true,
  "semicolons": true,
  "sortOrder": 'alphabetical',
  "stackedProperties": true,
  "universal": true,
  "valid": true,
  "whitespace": true,
  "zeroUnits": true,
  "zIndexDuplicates": true,
  "zIndexNormalize": false,
  "prefixVarsWithDollars": 'always',
}
